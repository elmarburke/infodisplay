<div class="tweet" id="{{id}}">
    <div class="left">
        <img src="{{profile_image_url}}">
    </div>
    <div class="right">
        <p><span class="user"><b>@{{from_user}}</b></span>&nbsp;<span class="username">{{from_user_name}}</span></p>
        <p class="body">{{text}}</p>
    </div>
</div>